body,
div,
h1,
h2,
h3,
h4,
h5,
h6,
p,
pre,
form,
input,
textarea,
th,
td,
ul,
ol,
li
  margin 0
  padding 0
html, body, a, button, input, select, textarea
  font: 12px/1.5 'Open Sans',Arial,Helvetica,sans-serif
  color #55595b
  -webkit-font-smoothing antialiased
  -webkit-tap-highlight-color transparent
  //-webkit-user-select none
body
  width: 100%;
  height: 100%;
  background: #f6f6f6;
  min-width: 1200px;
  overflow: auto;
ul, ol
  list-style none
table
  border-collapse collapse
  border-spacing 0
h1,
h2,
h3,
h4,
h5,
h6
  // font-size 100%
  font-weight normal
